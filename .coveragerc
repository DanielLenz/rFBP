[run]
branch = True
omit =
    */ReplicatedFocusingBeliefPropagation/__init__.py
    */ReplicatedFocusingBeliefPropagation/__version__.py
    */ReplicatedFocusingBeliefPropagation/rfbp/test/*
    */ReplicatedFocusingBeliefPropagation/example/*
[report]
# Regexes for lines to exclude from consideration
exclude_lines =
    pragma: no cover

    # Don't complain if tests don't hit defensive assertion code:
    raise AssertionError
    raise NotImplementedError

    # Don't complain if non-runnable code isn't run:
    if 0:
    if __name__ == .__main__.:

